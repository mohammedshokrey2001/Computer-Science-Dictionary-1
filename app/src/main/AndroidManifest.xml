<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.csdict">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ll"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ll"
        android:supportsRtl="true"
        android:theme="@style/Theme.CSdict">
        <activity
            android:name=".messageApp.MessageProfile"
            android:exported="true" />
        <activity
            android:name=".messageApp.Messages"
            android:exported="true"
            android:label="@string/title_activity_messages"
            android:theme="@style/Theme.CSdict.NoActionBar" />
        <activity
            android:name=".main.ResetPassword"
            android:exported="true" />
        <activity
            android:name=".main.WordProfileAdminView"
            android:exported="true" />
        <activity
            android:name=".AdminWoork.UserProfileAdminView"
            android:exported="true" />
        <activity
            android:name=".AdminWoork.AdminWork"
            android:exported="true"
            android:label="@string/title_activity_admin_work"
            android:theme="@style/Theme.CSdict.NoActionBar" />
        <activity
            android:name=".main.UserWork"
            android:exported="true"
            android:label="@string/title_activity_user_work"
            android:theme="@style/Theme.CSdict.NoActionBar" />
        <activity
            android:name=".main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.SignIn"
            android:exported="true" />
        <activity
            android:name=".main.SignUp"
            android:exported="true" />
    </application>

</manifest>